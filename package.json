{
  "name": "ui5-lib-visualization",
  "version": "0.3.7",
  "scripts": {
    "test": "yarn eslint ./src/ui5/viz  --ext .js --cache ",
    "start": "NODE_ENV=development DEV_PORT=3000 babel-node node_modules/gulp/bin/gulp.js --silent",
    "start:verbose": "NODE_ENV=development DEV_PORT=3000 babel-node node_modules/gulp/bin/gulp.js",
    "start:dist": "NODE_ENV=production DEV_PORT=3000 babel-node node_modules/gulp/bin/gulp.js testDist",
    "build": "NODE_ENV=production babel-node node_modules/gulp/bin/gulp.js build --silent",
    "build:verbose": "NODE_ENV=production babel-node node_modules/gulp/bin/gulp.js build",
    "build:disclaimer": "yarn licenses generate-disclaimer --production >> DISCLAIMER.md",
    "docs": "babel-node docs/tools/create-ui5-docs.js",
    "prepublish": "yarn test && yarn docs && yarn build && yarn build:disclaimer",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --no-semi --single-quote",
      "git add"
    ]
  },
  "description": "UI5 Visualization Library",
  "repository": "https://github.com/pulseshift/ui5-lib-visualization.git",
  "author": "Jascha Quintern <jascha.quintern@pulseshift.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pulseshift/ui5-lib-visualization/issues"
  },
  "homepage": "https://github.com/pulseshift/openui5-gulp-starter-kit#readme",
  "main": "src/demo/index.hbs",
  "ui5": {
    "indexTitle": "UI5 Visualization Library",
    "build": {
      "cacheBuster": false,
      "compression": false
    },
    "src": "cdn",
    "srcLinks": {
      "cdn": {
        "url": "https://openui5.hana.ondemand.com/{{version}}/resources/sap-ui-core.js",
        "version": "1.50.8",
        "isPrebuild": true,
        "isArchive": false
      }
    },
    "vendor": {
      "name": "ui5.viz.libs",
      "path": "src/ui5/viz/libs"
    },
    "theme": "sap_belize_plus",
    "apps": [],
    "libraries": [
      {
        "name": "ui5.viz",
        "path": "src/ui5/viz"
      }
    ],
    "themes": [],
    "assets": [
      {
        "name": "ui5.demo",
        "path": "src/demo"
      }
    ]
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^8.0.0",
    "browser-sync": "^2.18.13",
    "browserify": "^16.1.1",
    "connect-gzip-static": "^2.1.1",
    "del": "^4.0.0",
    "dox": "^0.9.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "fs-es6-promise": "^0.0.3",
    "gulp": "4.0",
    "gulp-babel": "^7.0.0",
    "gulp-brotli": "^1.2.1",
    "gulp-clean-css": "^4.0.0",
    "gulp-favicons": "^2.2.7",
    "gulp-gzip": "^1.4.2",
    "gulp-handlebars-html": "^0.0.2",
    "gulp-htmlmin": "^5.0.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^4.0.0",
    "gulp-install": "^1.1.0",
    "gulp-less": "^4.0.0",
    "gulp-main-npm-files": "^1.0.5",
    "gulp-order": "^1.1.1",
    "gulp-plumber": "^1.1.0",
    "gulp-pretty-data": "^0.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sequence": "^1.0.0",
    "gulp-tap": "^1.0.1",
    "gulp-touch-cmd": "^0.0.1",
    "gulp-uglify": "^3.0.0",
    "gulp-ui5-preload": "^1.3.1",
    "gulp-util": "^3.0.8",
    "handlebars": "^4.0.11",
    "less-plugin-autoprefix": "^2.0.0",
    "ora": "^3.0.0",
    "ui5-cache-buster": "0.1.1",
    "ui5-lib-util": "^0.1.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "2.0.0"
  },
  "dependencies": {
    "c3": "https://github.com/pulseshift/c3#master",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0"
  }
}
